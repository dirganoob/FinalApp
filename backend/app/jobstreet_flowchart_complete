// Flowchart of JobStreet Job Search System Using K-Means Clustering
digraph {
	A [label=Start shape=ellipse]
	B [label="Load Data (Job Listings)" shape=box]
	C [label="Preprocess Data (Normalize and Clean)" shape=box]
	D [label="Apply K-Means Clustering" shape=box]
	E [label="Assign Job Categories to Users" shape=box]
	F [label="Generate Job Recommendations" shape=box]
	G [label="Evaluate Recommendation Accuracy" shape=diamond]
	H [label=End shape=ellipse]
	A -> B
	B -> C
	C -> D
	D -> E
	E -> F
	F -> G
	G -> H
}
